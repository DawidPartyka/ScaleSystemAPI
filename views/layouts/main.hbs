<html lang="en">
<head>
    <!-- <meta> tags> -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    {{#each this.css}}<link rel="stylesheet" href="{{this}}">
    {{/each}}
    <title>System skal</title>
</head>

<body>
<div id="main">
    {{>header}}

    {{#if this.user}}
        {{>menu}}
    {{/if}}


    {{#if this.user.management.role}}
        {{>menumanagement}}
    {{/if}}

    {{{body}}}
    {{>footer}}
</div>

{{#each this.js}}<script src="{{this}}"></script>
{{/each}}

{{#each this.jsModules}}<script type="module" src="{{this}}"></script>
{{/each}}

</body>

<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
<!-- Bootstrap Date-Picker Plugin -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/js/bootstrap-datepicker.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/css/bootstrap-datepicker3.css"/>

<script src="https://kit.fontawesome.com/2db52e71cc.js" crossorigin="anonymous"></script>
</html>