<div class="bg-light align-items-left text-left col p-3 ml-1 rounded">
    <h2 class="text-info">Create time signature</h2>
    <form id="createTimeSignatureForm" onsubmit="return false">
        <div class="form-group">
            <label for="createTimeSignature">Time Signature name</label>
            <input type="text" class="form-control" id="createTimeSignature" aria-describedby="createTimeSignatureHelp" placeholder="Enter Time Signature name">
            <small id="createTimeSignatureHelp" class="form-text text-muted"></small>
        </div>

        <button id="submitCreateTimeSignature" class="btn btn-primary">Submit</button>
    </form>

    <div class="container">
        <p class="text-primary">Status: </p>
        <p id="createTimeSignatureStatus" class="text-info"></p>
        <p class="text-primary">Response content:</p>
        <pre>
                    <code id="createTimeSignatureResponse">
                    </code>
                </pre>
    </div>
</div>