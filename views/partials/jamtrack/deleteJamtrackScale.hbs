<div class="bg-light align-items-left text-left col p-3  ml-1 rounded">
    <h3 class="text-info">Delete jamtrack - scale relation</h3>
    <form id="deleteJamtrackScaleForm" onsubmit="return false">
        <div class="form-group">
            <label for="jamtrackScaleDeleteId">Jamtrack id</label>
            <input type="text" class="form-control" id="jamtrackScaleDeleteId" aria-describedby="deleteScaleIdHelp" required placeholder="Enter jamtrack id">
            <small id="deleteScaleIdHelp" class="form-text text-muted">Jamtrack id from which scale is meant to be deleted</small>
        </div>

        <div class="form-group">
            <label for="scaleDeleteId">Scale id</label>
            <input type="text" class="form-control" id="scaleDeleteId" aria-describedby="scaleDeleteIdHelp" required placeholder="Enter scale id">
            <small id="scaleDeleteIdHelp" class="form-text text-muted">Scale id meant to be deleted</small>
        </div>

        <button id="submitDeleteJamtrackScale" type="submit" class="btn btn-primary">Submit</button>
    </form>

    <div class="container">
        <p class="text-primary">Status: </p>
        <p id="deleteJamtrackScaleStatus" class="text-info"></p>
        <p class="text-primary">Response content:</p>
        <pre>
                    <code id="deleteJamtrackScaleResponse">
                    </code>
                </pre>
    </div>
</div>